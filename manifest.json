{
  "name": "Alpha Cheap",
  "version": "0.1",
  "description": "Finding deals and gathering price data",
  "manifest_version": 2,
  "permissions": ["storage", "contextMenus", "tabs",  "activeTab", "geolocation", "https://mflation.herokuapp.com/api"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Alpha Cheap"
  },
  "background": {
    "scripts": ["background.js"]
  }
}